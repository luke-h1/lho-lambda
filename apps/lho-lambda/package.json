{
  "name": "@nowplaying/lambda",
  "version": "1.3.0",
  "scripts": {
    "build": "esbuild --bundle --minify --keep-names --sourcemap --sources-content=false --target=node22 --platform=node --outfile=dist/index.js src/index.ts",
    "clean": "rm -rf ./dist",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "ts:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.24.2",
    "eslint": "8.57.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "aws-xray-sdk": "^3.10.2",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2"
  }
}